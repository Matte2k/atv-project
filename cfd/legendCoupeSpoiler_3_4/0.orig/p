FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "include/initialConditions"

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform $pressure;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    inlet
    {
        type            zeroGradient;
    }

    outlet
    {
        type            fixedValue;
        value           $internalField;
    }

    ground
    {
        type            zeroGradient;
    }

    COUPE_SPOILER
    {
        type            zeroGradient;
    }

    COUPE_UPPER_BODY
    {
        type            zeroGradient;
    }

    COUPE_LOWER_BODY
    {
        type            zeroGradient;
    }

    symmetry
    {
        type            symmetry;
    }

    COUPE_FRONT_WHEEL
    {
        type           zeroGradient;
    }

    COUPE_REAR_WHEEL
    {
        type           zeroGradient;
    }


    #include "include/BackUpperPatches"
}


